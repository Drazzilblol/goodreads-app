<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomRatingBar">
        <attr name="drawable_margin" format="dimension" />
        <attr name="drawable_size" format="dimension" />
        <attr name="max_count" format="integer" />
        <attr name="rating" format="float" />
        <attr name="is_indicator" format="boolean" />
        <attr name="drawable_empty" format="reference" />
        <attr name="drawable_filled" format="reference" />
        <attr name="drawable_half" format="reference" />
        <attr name="star_color" format="color" />
    </declare-styleable>

    <declare-styleable name="Dots">
        <attr name="dots_margin" format="dimension" />
        <attr name="dots_size" format="dimension" />
        <attr name="count" format="integer" />
        <attr name="checked_color" format="color" />
        <attr name="unchecked_color" format="color" />
    </declare-styleable>
</resources>